#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/{
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp KP_N7   &kp KP_N8   &mo 1       &kp KP_N7   &kp KP_N8   &mo 1       &kp KP_N7   &kp KP_N8   &mo 1       &kp none
                &kp KP_N4   &kp KP_N5   &kp KP_N6   &kp KP_N4   &kp KP_N5   &kp KP_N6   &kp KP_N4   &kp KP_N5   &kp KP_N6   &kp KP_N4
